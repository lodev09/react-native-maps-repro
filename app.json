{
  "expo": {
    "name": "react-native-maps-repro",
    "slug": "react-native-maps-repro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "reactnativemapsrepro",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reactnativemapsrepro",
      "config": {
        "googleMapsApiKey": "AIzaSyBeaou3cjmq_BED6HlTRdg5fSyArUCzoTM",
        "usesNonExemptEncryption": false,
      },
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.reactnativemapsrepro",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBeaou3cjmq_BED6HlTRdg5fSyArUCzoTM",
        },
      },
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "AIzaSyBeaou3cjmq_BED6HlTRdg5fSyArUCzoTM",
          "androidGoogleMapsApiKey": "AIzaSyBeaou3cjmq_BED6HlTRdg5fSyArUCzoTM"
        },
      ],
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
